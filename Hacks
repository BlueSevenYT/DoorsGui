    wait()
    fadeInThing(caveModel,1.5)

    -- move the cave model
    caveModel:PivotTo(CFrame.new(Vector3.new(0, -1.3, 3250, 1, 0, 0, 0, 1, 0, 0, 0, 1)) * CFrame.Angles(math.rad(-90), 0, 0))
    -- Npc 1
