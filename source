local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("BlueSeven's Hacks!", "Ocean")

local Tab = Window:NewTab("Player")

local Section = Tab:NewSection("Player Settings")

Section:NewSlider("Walkspeed", "Changes your walkspeed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewSlider("JumpPower", "Changes your jump power", 40, 20, function(p5) -- 40 (MaxValue) | 20 (MinValue)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = p5
end)

Section:NewKeybind("Hide UI Button", "button to hide ui", Enum.KeyCode.LeftControl, function()
	Library:ToggleUI()
end)

Section:NewButton("Give Crucifix (works on anything)", "Crucifix that works on anything!", function()
_G.Uses = 9999999
_G.Range = 30
_G.OnAnything = true
_G.Fail = false
loadstring(game:HttpGet('https://raw.githubusercontent.com/PenguinManiack/Crucifix/main/Crucifix.lua'))()
 print("Clicked")
end)

