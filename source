NoClip.TextWrapped = true

UICorner.Parent = NoClip

injected.Name = "injected"
injected.Parent = Main
injected.BackgroundColor3 = Color3.fromRGB(52, 255, 21)
injected.Position = UDim2.new(0,0,00)
